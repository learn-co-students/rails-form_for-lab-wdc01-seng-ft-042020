<%# 
Form_for @snack do |f|
		f.label :name
		f.text_field :name
		f.label :calories
		f.number_field :calories
		f.label :deliciousness
		f.select :deliciousness, (1..10)
		f.submit :snackify
	end %>
<h1>Student Form</h1>

<%=form_for(@student) do |f|%>
    <%= f.label :first_name %>
    <%= f.text_field :first_name%>
    <%= f.label :last_name%>
    <%= f.text_field :last_name %>
    <%= f.submit%>
<%end%>
